<!DOCTYPE html>
<html lang="en">
    <%- include ("../fragmente/head") -%>
<body>
    <%- include ("../fragmente/header") -%>
    <div class="spatiu-gol">
    </div>
    <div class="container-mobile container d-flex flex-column  justify-content-center">
        <% if(lungime != 0) { %>
            <div class="d-flex align-items-center justify-content-around">
                <h5 class="display-6">Cos: <span style="color: red"><%-lungime %></span></h5> 
                <a href="/cos" class="custom-button -green">Vizualizare </a>
            </div>
        <% } %>
            <form action="/search" class="d-flex" method="GET">
                <input name="key" class="form-control me-2" type="search" placeholder="Cauta investigatii" aria-label="Search">
                <button class="btn btn-outline-success" type="submit">Search</button>
            </form>
        
    </div>
    <div class="container">
        <h6 class="display-6"> Ati cautat: <%=cautare%></h6>
        <div class="row align-items-stretch d-flex justify-content-center">
            <% for(let analiz of locals.analize) { %>
                <div class="col-xl-3 col-lg-4 col-md-6 col-sm-6 col-xs-10" style="margin-top:15px;">
                    <div class="d-flex flex-column items analiza card p-3 bg-white" style="margin: 10px 5px 10px 5px">
                        <div class="about-product text-center mt-2">
                            <div>
                                <h4 class="display-6" style="color:rgb(31, 179, 110); font-size: 30px;"><%- analiz.nume %></h4>
                                <h6 class="mt-0 text-black-50"></h6>
                            </div>
                        </div>
                        <div class="stats mt-2">
                            <!-- <div class="d-flex justify-content-between p-price"><span>Timp de executie:</span><span><%- analiz.timpexecutie %></span></div>
                            <div class="d-flex justify-content-between p-price"><span>Tip analiza:</span><span></span><%- analiz.tipanaliza %></div> -->
                            <div class="p-price"><center><span>Pret:</span><span style='color:rgb(228, 35, 35)'><b><%- analiz.pret %> RON</b></span></center></div>
                        </div>
                        <!-- <div class="d-flex justify-content-between total font-weight-bold mt-4"><span>Total</span><span>$7,197.00</span></div> -->
                        <center style="margin-top: auto;">
                            <form action="/analize/adauga/<%-analiz.id_analize%>" method="POST">
                                <input type="submit" class="custom-button -green"  value="Adauga">
                            </form>
                            <!-- <a href="" class="custom-button anotherblue"> Mai multe informatii </a> -->
                        </center>
                    </div>
                </div>
            <% } %>
            <% for(let consult of locals.consultatii) { %>
                <div class="col-xl-3 col-lg-4 col-md-6 col-sm-6 col-xs-10" style="margin-top:15px;">
                    <div class="d-flex flex-column items analiza card p-3 bg-white" style="margin: 10px 5px 10px 5px">
                        <div class="about-product text-center mt-2">
                            <div>
                                <h4 class="display-6" style="color:rgb(31, 179, 110); font-size: 30px;"><%- consult.nume %></h4>
                                <h6 class="mt-0 text-black-50"></h6>
                            </div>
                        </div>
                        <div class="stats mt-2">
                            <div class="p-price"><center><span>Pret:</span><span style='color:rgb(228, 35, 35)'><b><%- consult.pret %> RON</b></span></center></div>
                        </div>
                        <!-- <div class="d-flex justify-content-between total font-weight-bold mt-4"><span>Total</span><span>$7,197.00</span></div> -->
                        <center style="margin-top: auto;">
                            <form action="/consultatii/adauga/<%-consult.id_consultatie%>" method="POST">
                                <input type="submit" class="custom-button -green"  value="Adauga">
                            </form>
                            <!-- <a href="" class="custom-button anotherblue"> Mai multe informatii </a> -->
                        </center>
                    </div>
                </div>
            <% } %>
            <% for(let ecog of locals.ecografii) { %>
                <div class="col-xl-3 col-lg-4 col-md-6 col-sm-6 col-xs-10" style="margin-top:15px;">
                    <div class="d-flex flex-column items analiza card p-3 bg-white" style="margin: 10px 5px 10px 5px">
                        <div class="about-product text-center mt-2">
                            <div>
                                <h4 class="display-6" style="color:rgb(31, 179, 110); font-size: 30px;"><%- ecog.nume %></h4>
                                <h6 class="mt-0 text-black-50"></h6>
                            </div>
                        </div>
                        <div class="stats mt-2">
                            <div class="p-price"><center><span>Pret:</span><span style='color:rgb(228, 35, 35)'><b><%- ecog.pret %> RON</b></span></center></div>
                        </div>
                        <!-- <div class="d-flex justify-content-between total font-weight-bold mt-4"><span>Total</span><span>$7,197.00</span></div> -->
                        <center style="margin-top: auto;">
                            <form action="/ecografii/adauga/<%-ecog.id_ecografie%>" method="POST">
                                <input type="submit" class="custom-button -green"  value="Adauga">
                            </form>
                            <!-- <a href="" class="custom-button anotherblue"> Mai multe informatii </a> -->
                        </center>
                    </div>
                </div>
            <% } %>
        </div>
        
    </div>
    <%- include ("../fragmente/footer") -%>
</body>
</html>